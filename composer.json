{
    "name": "codexsoft/transmission",
    "license": "MIT",
    "description": "PHP Library for building HTTP JSON API",
    "authors": [
        {
            "name": "Dmitriy V Kozubskiy",
            "email": "kozubsky@inbox.ru",
            "homepage": "http://codexsoft.ru/",
            "role": "Maintainer"
        }
    ],
    "support": {
        "issues": "https://github.com/codexsoft/transmission/issues",
        "source": "https://github.com/codexsoft/transmission"
    },
    "require": {
        "php": "^7.4.0",
        "ext-json": "*",
        "symfony/validator": "^5.1",
        "symfony/http-foundation": "^4.3|^5.0",
        "symfony/finder": "^4.4|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "CodexSoft\\Transmission\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodexSoft\\Transmission\\": ["tests/unit/"],
            "": ["vendor/"]
        }
    }
}
